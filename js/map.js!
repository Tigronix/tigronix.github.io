ymaps.ready(init);

function init(){
	var myMap = new ymaps.Map('map', {
	center: [55.882339, 37.725829],
	zoom: 11,
	controls: ["zoomControl"]
});

// отключить zoom колесиком
myMap.behaviors.disable("scrollZoom"), window.innerWidth < 480 && myMap.behaviors.disable("drag");


function checkState(){
	var shownObjects,
	byName = new ymaps.GeoQueryResult();

	if (jQuery('#car').prop('checked')){byName = myObjects.search('properties.name = "car"');}
	if (jQuery('#bus').prop('checked')){byName = myObjects.search('properties.name = "bus"').add(byName);}
	if (jQuery('#train').prop('checked')){byName = myObjects.search('properties.name = "train"').add(byName);}

	// Показываем объекты на карте
	shownObjects = byName.addToMap(myMap);
	// Объекты, которые не попали в выборку, нужно убрать с карты.
	myObjects.remove(shownObjects).removeFromMap(myMap);
}


jQuery('#OneLink').click(checkState);
jQuery('#TwoLink').click(checkState);
jQuery('#ThreeLink').click(checkState);

window.myObjects = ymaps.geoQuery({
type: "FeatureCollection",
features:[

